
# Please have the following environment variables:
#
# AWS_ACCESS_KEY_ID
# AWS_SECRET_ACCESS_KEY
# RS_HOST
# RS_PORT
# RS_DB
# RS_USER
# RS_PASS



docker run -it \
  -e AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID} \
  -e AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY} \
  -e PGHOST=${RS_HOST}   \
  -e PGPORT=${RS_PORT}   \
  -e PGDATABASE=${RS_DB} \
  -e PGUSER=${RS_USER}   \
  -e PGPASSWORD=${RS_PASS}   \
  --rm dewey "$@"
